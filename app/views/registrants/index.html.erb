<h1>Listing registrants</h1>

<table>
  <tr>
    <th>First name</th>
    <th>Last name</th>
    <th>Birth date</th>
    <th>Ssn</th>
    <th>E mail</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @registrants.each do |registrant| %>
  <tr>
    <td><%= registrant.first_name %></td>
    <td><%= registrant.last_name %></td>
    <td><%= registrant.birth_date %></td>
    <td><%= registrant.ssn %></td>
    <td><%= registrant.e_mail %></td>
    <td><%= link_to 'Show', registrant %></td>
    <td><%= link_to 'Edit', edit_registrant_path(registrant) %></td>
    <td><%= link_to 'Destroy', registrant, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Registrant', new_registrant_path %>
